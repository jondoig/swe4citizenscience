                        </swe:Quantity>
                    </swe:field>
                    <swe:field name="measurementAccuracy">
                        <swe:QuantityRange>
                            <swe:uom code="%"/>
                            <swe:value>-5 5</swe:value>
                        </swe:QuantityRange>
                    </swe:field>
                </swe:DataRecord>
            </sml:capability>
        </sml:CapabilityList>

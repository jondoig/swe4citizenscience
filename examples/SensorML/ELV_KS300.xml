                        </swe:QuantityRange>
                    </swe:field>
                    <swe:field name="measurementResolution">
                        <!-- how to express conditional values here? -->
                        <swe:QuantityRange>
                            <swe:uom/>
                        </swe:QuantityRange>
                    </swe:field>
                </swe:DataRecord>
            </sml:capability>
            <sml:capability name="rainSensor">
                <swe:DataRecord>
                    <swe:field name="measurementRange" xlink:href="#rainSensor">
                        <swe:QuantityRange>
                            <swe:uom code="mm"/>
                            <swe:value>0 999</swe:value>
                        </swe:QuantityRange>
                    </swe:field>
                    <swe:field name="measurementResolution">
                        <swe:QuantityRange>
                            <swe:uom code="mm"/>
                            <swe:value>0 0.3</swe:value>
                        </swe:QuantityRange>
                    </swe:field>
                </swe:DataRecord>
            </sml:capability>
            <sml:capability name="humiditySensor">
                <swe:DataRecord>
                    <swe:field name="measurementRange" xlink:href="#humiditySensor">
                        <swe:QuantityRange>
                            <swe:uom code="%"/>
                            <swe:value>0 99</swe:value>
                        </swe:QuantityRange>
                    </swe:field>
                    <swe:field name="measurementResolution">
                        <swe:Quantity>

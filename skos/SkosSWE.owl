        <skos:broader rdf:resource="http://www.opengeospatial.org/skos/swe#ObservedProperty"/>
    </owl:NamedIndividual>
    


    <!-- http://www.opengeospatial.org/skos/swe#RelativeHumidity -->

    <owl:NamedIndividual rdf:about="http://www.opengeospatial.org/skos/swe#RelativeHumidity">

    <owl:NamedIndividual rdf:about="http://www.opengeospatial.org/skos/swe#TransmitterFrequency">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition xml:lang="en">Radio frequency used by the device</skos:definition>
        <skos:broader rdf:resource="http://www.opengeospatial.org/skos/swe#OperatingProperty"/>

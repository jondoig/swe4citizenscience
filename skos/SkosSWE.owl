    <!-- http://www.opengeospatial.org/skos/swe#AirPressure -->

    <owl:NamedIndividual rdf:about="http://www.opengeospatial.org/skos/swe#AirPressure">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition xml:lang="en">The pressure asserted by the mass of the column of air directly above any specific point.</skos:definition>
        <skos:exactMatch rdf:resource="http://www.opengeospatial.org/skos/swe#AtmosphericPressure"/>
        <skos:broader rdf:resource="http://www.opengeospatial.org/skos/swe#ObservedProperty"/>
